<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="DurationPickerPreference">
        <attr name="maxExternal" format="string"/>
        <attr name="minExternal" format="string"/>

        <attr name="hourMax" format="integer"/>
        <attr name="hourMin" format="integer"/>

        <attr name="minuteMax" format="integer"/>
        <attr name="minuteMin" format="integer"/>

        <attr name="secondMax" format="integer"/>
        <attr name="secondMin" format="integer"/>

        <attr name="pickers">
            <flag name="hour" value="0x4"/>
            <flag name="minute" value="0x2"/>
            <flag name="second" value="0x1"/>
        </attr>
    </declare-styleable>
</resources>
